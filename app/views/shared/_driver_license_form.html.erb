<%= form_for(@driver_license) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.label :code  %>
    <%= f.text_field :code  %>
    <%= f.label :expiration  %>
    <%= f.date_field :expiration  %>
  </div>
  <%= f.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>
