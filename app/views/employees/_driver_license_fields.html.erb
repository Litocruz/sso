<%= render 'shared/error_messages', object: f.object %> 
<div class="control-group">
	<label for="code" class="control-label">Categoria</label>
	<div class="controls">
	    <%= f.select :driver_license_code_id, placeholder: "Categoria", class: "input-xlarge" %>
	</div>
</div>
<div class="control-group">
	<label for="expiration" class="control-label">Fecha Vencimiento</label>
	<div class="controls">
	    <%= f.date_select :expiration, class: "input-xlarge" %>
	</div>
</div>
<div class="control-group">
	<label for="description" class="control-label">Descripción</label>
	<div class="controls">
	    <%= f.text_field :description, placeholder: "Descripción", class: "input-xlarge" %>
	</div>
</div>
<hr>
	
