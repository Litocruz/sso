<% provide(:title, 'Alta Administrador') %>
<div class="page-header">
    <h1 class="pull-left">Empleados</h1>
</div>
<div class="row-fluid">
  <div class="span12">
    <%= form_for(@user) do |f| %>
      <div class"input-group input-lg">
        <%= render 'fields', f: f %>
        <%= f.submit "Crear Administrador", class: "btn btn-large btn-primary" %>
      </div>
    <% end %> 
  </div>
</div>
